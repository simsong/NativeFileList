{
	"name": "NativeFileList",
	"author": [
		"Simson Garfinkel"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:NativeFileList",
	"descriptionmsg": "native-file-list-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.32.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\NativeFileList\\": "includes/"
	},
	"AutoloadClasses": {
		"NativeFilesRecentlyAdded": "includes/NativeFilesRecentlyAdded.php",
		"NativeFilesRecentlyChanged": "includes/NativeFilesRecentlyChanged.php",
		"NativeFilesRecentlyDeleted": "includes/NativeFilesRecentlyDeleted.php"
	},
	"config": {
		"NativeFileListEnableTalk": {
			"description": "Enable the Talk feature for each file.",
			"value": false
		},
		"NativeFileListVandalizeEachPage": {
			"description": "Write 'NativeFileList was here' on each page",
			"value": false
		}
	},
	"Hooks": {
		"SpecialSearchResultsAppend": "MediaWiki\\Extension\\NativeFileList\\Hooks::onSpecialSearchResultsAppend",
		"LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\NativeFileList\\Hooks::onLoadExtensionSchemaUpdates"
	},
	"SpecialPages": {
		"NativeFilesRecentlyAdded": "NativeFilesRecentlyAdded",
		"NativeFilesRecentlyChanged": "NativeFilesRecentlyChanged",
		"NativeFilesRecentlyDeleted": "NativeFilesRecentlyDeleted"
	},
	"AvailableRights": [
		"nativefilelist-right"
	],
	"MessagesDirs": {
		"NativeFileList": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"NativeFileListAlias": "i18n/NativeFileList.i18n.alias.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "NativeFileList/resources"
	},
	"ResourceModules": {
		"ext.boilerPlate": {
			"dependencies": [],
			"messages": [],
			"styles": ["modules/NativeFileList.css"],
			"scripts": []
		}
	},
	"manifest_version": 2
}
